{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "~/*": [ "./*" ]
    },
    "baseUrl": "./src",
  },
  "rootDirs": ["./src"],
  "include": [
    "./src/**/*.ts",
    "./src/**/*.tsx"
  ],
  "exclude": [
    "node_modules", "babel.config.js"
  ]
}
